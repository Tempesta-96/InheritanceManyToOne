<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="institution.Staff" table="Staff">
        <id name="s_code" type="int"/>
        <property name="s_name" column="NAME" />
        <property name="specialization" column="SUBJECT"/>
		<property name="centre_name"/>
		
     </class>


</hibernate-mapping>